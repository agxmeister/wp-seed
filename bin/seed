#!/usr/local/bin/php
<?php

require __DIR__ . '/../vendor/autoload.php';

use Minicli\App;
use Minicli\Command\CommandCall;

$app = new App();
$app->setSignature('./seed get-different-paths');

$app->registerCommand('get-different-paths', function(CommandCall $input) {
    var_dump($input);
});

try {
    $app->runCommand($argv);
} catch (Throwable $e) {
    echo 'An error occurred running the command: ',  $e->getMessage(), "\n";
}
