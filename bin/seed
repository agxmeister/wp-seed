#!/usr/local/bin/php
<?php

require __DIR__ . '/../vendor/autoload.php';

use Minicli\App;

$app = new App();
$app->setSignature('./seed get-different-paths');

$commands = require __DIR__ . '/../app/commands.php';
$commands($app);

try {
    $app->runCommand($argv);
} catch (Throwable $e) {
    echo 'An error occurred running the command: ',  $e->getMessage(), "\n";
}
